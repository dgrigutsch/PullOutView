<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="PullOutAttrs">
        <attr name="pov_shadowDrawable" format="boolean"/>
        <attr name="pov_shadowWidth" format="dimension"/>
        <attr name="pov_closeOnTapEnabled" format="boolean"/>
        <attr name="pov_openOnTapEnabled" format="boolean"/>
        <attr name="pov_offsetWidth" format="dimension" />
        <attr name="pov_minHeight" format="dimension" />
        <attr name="pov_offsetTop" format="dimension" />
        <attr name="pov_stickTo" format="enum">
            <enum name="auto" value="0"/>
            <enum name="right" value="-1"/>
            <enum name="left" value="-2"/>
            <enum name="middle" value="-3"/>
            <enum name="top" value="-4"/>
            <enum name="bottom" value="-5"/>
        </attr>
    </declare-styleable>

</resources>